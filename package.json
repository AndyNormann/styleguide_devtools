{
  "name": "styleguide_devtools",
  "version": "0.5.0",
  "description": "Oslo Styleguide devtools",
  "main": "index.js",
  "scripts": {
    "hmr": "webpack-dev-server --hot --config config/webpack.app.js",
    "dev": "rm -rf dist/* && npm run build-structure \"false\" && php src/utils/twigCompiler.php && webpack --config config/webpack.modules.js --mode development && npm run build-structure \"true\"",
    "dev-modules": "webpack --config config/webpack.modules.js --mode development",
    "build-structure": "./node_modules/.bin/babel-node src/utils/buildAtomicStructure.js",
    "build-modules": "webpack --config config/webpack.modules.js --mode production",
    "build-viewer": "webpack --config config/webpack.app.js --mode production",
    "build-production": "rm -rf dist/* && npm run build-structure \"false\" && php src/utils/twigCompiler.php && npm run build-modules && npm run build-structure \"true\" && npm run build-viewer",
    "mkpat": "babel-node --presets env src/utils/newPattern.js",
    "watch": "onchange -i --no-exclude \"node_modules/ok-atomic-frontend/src/**/*\" -- ./node_modules/.bin/babel-node eventListener.js {{event}} {{changed}}",
    "alt-watch": "onchange -i --no-exclude \"node_modules/styleguide/src/**/*\" -- npm run dev",
    "hmr-watch": "npm run dev && concurrently \"npm run hmr\" \"npm run alt-watch\" --kill-others --kill-others-on-fail"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/oslokommune/styleguide_devtools.git"
  },
  "author": "Lars Boldt",
  "license": "ISC",
  "homepage": "https://github.com/oslokommune/styleguide_devtools#readme",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.4.2",
    "axe-core": "^3.1.2",
    "bulma": "^0.7.2",
    "dotenv-webpack": "^1.5.7",
    "font-face-mixin": "^1.1.0",
    "marked": "^0.5.1",
    "minireset.css": "0.0.3",
    "vue": "^2.5.17",
    "vue-color": "^2.7.0",
    "vue-highlightjs": "^1.3.3",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.17",
    "vuex": "^3.0.1",
    "vuex-persistedstate": "^2.5.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6",
    "babel-loader": "^7",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "concurrently": "^4.0.1",
    "copy-webpack-plugin": "^4.5.4",
    "css-loader": "^1.0.1",
    "dotenv": "^6.1.0",
    "eslint": "^5.8.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-vue": "^5.0.0-beta.3",
    "file-loader": "^2.0.0",
    "fs": "^0.0.1-security",
    "github-markdown-css": "^2.10.0",
    "glob": "^7.1.3",
    "hard-source-webpack-plugin": "^0.12.0",
    "html-webpack-plugin": "^3.2.0",
    "inquirer": "^6.2.0",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.9.4",
    "npm": "^6.4.1",
    "onchange": "^5.0.2",
    "resolve-url-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "vue-loader": "^15.4.2",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  }
}
